{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "types": ["node"],
    "lib": ["es2018", "dom"],
    "paths": {
      "mat-reduce": ["./dist/mat-reduce"],
      "mat-reduce/*": ["./dist/mat-reduce/*"],
      "mat-reduce-core": ["./dist/mat-reduce-core"],
      "mat-reduce-core/*": ["./dist/mat-reduce-core/*"]
    }
  }
}
