{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "../../out-tsc/demo",
    "types": [],
    "paths": {
      "mat-reduce": ["../mat-reduce/src/public_api"],
      "mat-reduce/*": ["../mat-reduce/src/*"],
      "mat-reduce-core": ["../mat-reduce-core/src/public_api"],
      "mat-reduce-core/*": ["../mat-reduce-core/src/*"]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "src/test.ts",
    "src/environments"
  ],
  "angularCompilerOptions": {
    "strictTemplates": true,
    "strictInjectionParameters": true
  }
}
